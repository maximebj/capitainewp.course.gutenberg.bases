{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "capitainewp/edit",
	"version": "1.1",
	"title": "3. Bloc éditable",
	"icon": "edit",
	"category": "text",
	"keywords": [ "Capitaine WP", "Gutenberg", "édition" ],
	"description": "Un bloc avec un contenu éditable",
	"supports": {
		"html": false
	},
	"attributes": {
		"content": {
			"type": "string",
			"source": "text",
			"selector": ".content"
		}
	},
	"textdomain": "capitainewp-gut-bases",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
