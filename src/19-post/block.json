{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "capitainewp/post",
	"version": "1.1",
	"title": "19. Bloc Post",
	"icon": "sticky",
	"category": "text",
	"description": "Un bloc qui affiche un article",
	"attributes": {
		"postID": {
			"type": "integer"
		},
		"showCategory": {
			"type": "boolean",
			"default": true
		},
		"showAuthor": {
			"type": "boolean",
			"default": true
		},
		"showImage": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "capitainewp-gut-bases",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
